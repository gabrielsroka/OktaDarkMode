{
  "short_name": "Okta Dark",
  "name": "Okta Dark Mode",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/logo-medium16.png",
    "48": "icons/logo-medium16.png",
    "128": "icons/logo-medium128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://*.oktapreview.com/*",
    "https://*.okta.com/*",
    "https://*.okta-emea.com/*"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Okta Dark"
  },
  "version": "1.0"
}
